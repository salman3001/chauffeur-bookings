    "@vueuse/core": "^10.11.0",
    "@vueuse/integrations": "^10.11.0",
    "apexcharts": "3.45.2",
    "axios": "^1.7.2",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21",
    "maska": "^1.5.0",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.7.5",
    "universal-cookie": "^6.1.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vue-tabler-icons": "2.21.0",
